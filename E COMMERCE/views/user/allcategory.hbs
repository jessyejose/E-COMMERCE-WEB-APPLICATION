<div class="container">
  <div class="row">
     {{#each commodityInfo}}
        <div class="col-sm-4">
          <div class="card" style="width: 18rem;">
            <img class="card-img-top" src="/images/{{this.Imagenew}}" alt="Card image" width="100px" height="300px">
            <div class="card-body">
              <h4 class="card-title">Category: {{category.Category}}</h4>
              <h5 class="card-title">Product: {{Product}}</h5>
              <h5 class="card-title">Price: {{Price}}</h5>
              <p class="card-text">Description: {{Description}}</p>
            </div>
          </div>
        </div> 
      {{/each}} 
  </div>
</div>
<a href="/" class="card-link btn btn-primary">Back</a>


